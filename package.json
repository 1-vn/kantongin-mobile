{
  "name": "csnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.android.js --bundle-output iOS/main.jsbundle --platform 'android' --assets-dest ./  --dev false --reset-cache",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "axios": "0.17.1",
    "base-64": "0.1.0",
    "date-fns": "1.29.0",
    "gettext.js": "git+https://github.com/freiserg/gettext.js.git",
    "lodash": "4.17.4",
    "query-string": "^5.0.1",
    "react": "16.1.1",
    "react-native": "0.50.3",
    "react-native-device-info": "0.12.1",
    "react-native-extended-stylesheet": "0.8.0",
    "react-native-navigation": "1.1.281",
    "react-native-scrollable-tab-view": "0.8.0",
    "react-native-splash-screen": "3.0.6",
    "react-native-swipeout": "2.3.1",
    "react-native-swiper": "1.5.13",
    "react-native-vector-icons": "4.4.2",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-persist": "4.10.1",
    "redux-thunk": "2.2.0",
    "shitty-qs": "1.0.1",
    "tcomb-form-native": "0.6.11"
  },
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "jest": "21.2.1",
    "nock": "^9.1.4",
    "po2json": "0.4.5",
    "react-gettext-parser": "1.3.0",
    "react-test-renderer": "16.1.1",
    "redux-mock-store": "^1.3.0"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFiles": [
      "<rootDir>/__tests__/mocks/global.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)"
    ]
  }
}
